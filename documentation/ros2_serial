# ros2 Serial
# https://github.com/osrf/ros2_serial_example


mkdir -p ~/Desktop/ros2_serial_example_ws/src
cd ~/Desktop/ros2_serial_example
/src
git clone https://github.com/osrf/ros2_serial_example.git
cd ..

colcon build --cmake-args -DROS2_SERIAL_PKGS="sensor_msgs;px4_msgs"
source install/local_setup.bash


